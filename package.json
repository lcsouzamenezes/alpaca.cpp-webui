{
  "name": "chatgpt-ui-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently --kill-others \"npm run proc_dev\" \"npm run proc_native\"",
    "proc_dev": "next dev",
    "proc_serve": "PORT=13000 next start",
    "proc_native": "node ./utils/native.js",
    "build": "next build",
    "start": "next build && concurrently --kill-others \"npm run proc_serve\" \"npm run proc_native\"",
    "lint": "next lint"
  },
  "dependencies": {
    "@next/font": "13.1.1",
    "axios": "^1.2.2",
    "dotenv": "^16.0.3",
    "eslint": "8.31.0",
    "eslint-config-next": "13.1.1",
    "express": "^4.18.2",
    "next": "13.1.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sequelize": "^6.30.0",
    "socket.io": "^4.6.1",
    "socket.io-client": "^4.6.1",
    "sqlite3": "^5.1.6",
    "underscore": "^1.13.6",
    "uuid": "^9.0.0",
    "uuidv4": "^6.2.13"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.13",
    "concurrently": "^8.0.1",
    "postcss": "^8.4.21",
    "tailwindcss": "^3.2.6"
  }
}
